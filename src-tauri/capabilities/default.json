{
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "shell:allow-open",
    "shell:default","opener:allow-open-path","opener:allow-open-url",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "server-manager-backend",
          "sidecar": true,
          "args": true
        }
      ]
    },
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "server-manager-backend",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
